
;; main section
;; whether run as a deamon process.
is_deamon=1

app_lock=1

;; message queue key
mq_key=1688

;; share memory key for process conf
conf_key=2688

;; the count of business instance
app_count=4

;; the number for each memory block
mq_bk_count=16


;; valid time for message in message queue
mq_validTime=600

mq_tryTime=5

mq_sleepTime=0

mq_SleepTimeu=5

client_len=1680

listen_len=128

client_timeout=60

admin_ip=127.0.0.1
admin_port=16888

admin_conf=../conf/sos.cfg
;;the path of sos
sos_path=../sos

;; server name
server_pathfile=./server
server_filename=server

;; sos path
sos_home=../sos

